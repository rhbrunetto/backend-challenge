{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "sku": {
      "type": "number",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "created_at": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "updated_at": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "protected": [
    "price",
    "created_at",
    "updated_at"
  ],
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
